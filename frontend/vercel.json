{
  "version": 2,
  "name": "cpg2pvg-ai-frontend",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_API_URL": "@api_url",
    "NEXT_PUBLIC_ENVIRONMENT": "production",
    "NEXT_PUBLIC_VERSION": "1.0.0"
  },
  "regions": ["hkg1", "sin1"],
  "functions": {
    "app/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
}